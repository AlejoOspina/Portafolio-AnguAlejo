.registro
    text-align center
    color black
    margin auto
    width 23%


form 
    position relative
    width 521px
    margin 15px auto 0 auto
    text-align center
    
.imagen-form
  position fixed
  top 0
  right 0
  width 100%
  background-img url('../../img/formulario.jpg')
  background-repeat no-repeat
  background-size cover
    
.Titulo-form
    color  black
    font-size 148%
    margin-top 1em

    
    
.Formulario
  text-align center
  margin-top 8em



.Form-input 
  width 270px
  height 42px
  margin-top 25px
  padding 0 15px
  background #2d2d2d /* browsers that don't support rgba */
  -moz-border-radius 6px
  -webkit-border-radius 6px
  border-radius 6px
  border: 1px solid #3d3d3d /* browsers that don't support rgba */
  border: 1px solid rgba(255,255,255,.15)
  -moz-box-shadow 0 2px 3px 0 rgba(0,0,0,.1) inset
  -webkit-box-shadow 0 2px 3px 0 rgba(0,0,0,.1) inset
  box-shadow 0 2px 3px 0 rgba(0,0,0,.1) inset
  font-family 'PT Sans', Helvetica, Arial, sans-serif
  font-size 14px
  color #fff
  text-shadow 0 1px 2px rgba(0,0,0,.1)
  -o-transition all .2s
  -moz-transition all .2s
  -webkit-transition all .2s
  -ms-transition all .2s
    
ul li
  float left
  margin 10px
  padding 2px
  z-index 1


ul a
  color #F3F3F3
  text-decoration none
  
header
  position fixed
  width 100%
  margin-top -8px
  height 70px
  background-color transparent
  z-index 1
  -webkit-transition background-color .5s
    


nav
  display table
  margin auto
  margin-top 5px
  height 40px
  -webkit-transition margin-left .5s
  

.Logo#logo
  float left
  display block
  margin-left 49px
  margin-right -27px
  margin-top 150px
  -webkit-transition width .5s, height .5s, margin-right .5s, margin-top .5s



ul
  display block
  float left
  list-style none
  
.Barra-navegacion
  width 100%
  height 100%
  overflow-x:hidden
  background: white
  
